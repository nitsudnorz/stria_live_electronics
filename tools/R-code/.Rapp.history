drawpage <- function (nr,beg, end)#
{#
  par(mfcol=c(8,1), mai = c(0, 1, 0, 0.4), omi = c(0.2, 0, 0.5, 0), cex = 0.5) # page setup#
  #plot(c(1:100))#
  #plotter(beg, end)#
  for (fader_CC in 7:0){#
  	drawfader(fader_CC, beg, end)#
    }#
