f <- function (l, r , x, y, a, o, c)# left right start, end, amplitude, offset=freq, c=color#
{#
  polygon((x+getx(l)*(y-x)/16384),(gety(l)*a+o), col = grey(c))#
  polygon((x+getx(r)*(y-x)/16384),(gety(r)*-1*a+o), col = grey(c))#
}#
