        f(stria_score$ampF[k],stria_score$IAF[k] ,stria_score$Start[k], stria_score$End[k], stria_score$Amp[k]*Amp_scale_fact, y+(12*log2(stria_score$Freq[k]/440)+69)*0.005, col_r)#
