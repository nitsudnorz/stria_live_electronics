	        text(stria_score$Start[k] + stria_score$Dur[k]*0.5,#
	             y+(12*log2(stria_score$Freq[k]/440)+69)*0.005,#
	             labels = paste(round(stria_score$Dur[k], 0), "s"), cex = 1.5, col = "white")#
